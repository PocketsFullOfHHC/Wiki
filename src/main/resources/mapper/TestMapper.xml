<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hhc.wiki.mapper.TestMapper" >

    <!--  上面的namespace对应着TestMapper.java的包加上接口名字，下面语句中的id对应TestMapper接口里面的函数名，resultType对应着实体列Test的JavaBean  -->
    <select id="list" resultType="com.hhc.wiki.domain.Test">
        select `id`, `name`, `password` from `test`
    </select>

</mapper>